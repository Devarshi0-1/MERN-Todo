{
    "version": 2,
    "rewrites": [
        {
            "source": "/(.*)",
            "destination": "/index.html"
        }
    ],
    "routes": [
        {
            "src": "/api/v1/users/(.*)",
            "dest": "/api/v1/users/$1"
        },
        {
            "src": "/api/v1/task/(.*)",
            "dest": "/api/v1/task/$1"
        },
        {
            "src": "/api/v1/admin/(.*)",
            "dest": "/api/v1/admin/$1"
        }
    ]
}
